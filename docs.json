{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "css": "/global.css",
  "name": "Arzule",
  "colors": {
    "primary": "#FFFFFF",
    "light": "#E5E5E5",
    "dark": "#000000"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "installation"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/traces",
              "concepts/events",
              "concepts/runs"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/claude-code",
              "integrations/crewai",
              "integrations/langgraph",
              "integrations/langchain",
              "integrations/autogen"
            ]
          },
          {
            "group": "Configuration",
            "pages": [
              "configuration/environment-variables",
              "configuration/self-hosting",
              "configuration/pii-redaction"
            ]
          }
        ]
      },
      {
        "tab": "SDK Reference",
        "groups": [
          {
            "group": "Python SDK",
            "pages": [
              "sdk/python/overview",
              "sdk/python/arzule-run",
              "sdk/python/sinks",
              "sdk/python/cli"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/ArzuleDev/arzule-ingest",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/arzule.png",
    "dark": "/logo/arzule.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@arzule.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.arzule.com"
    }
  },
  "appearance": {
    "default": "dark"
  },
  "font": {
    "headings": {
      "family": "Inter"
    },
    "body": {
      "family": "Inter"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/ArzuleDev/arzule-ingest",
      "linkedin": "https://www.linkedin.com/company/arzulehq/posts/?feedView=all"
    }
  },
  "whiteLabel": true
}
